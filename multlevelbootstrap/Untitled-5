CREATE TABLE `AD_DISPUTE_INFO` (
  `DISPUTE_ID` int(10) NOT NULL AUTO_INCREMENT,
  `VAN` varchar(100) NOT NULL,
  `AMOUNT` double(20,4) NOT NULL DEFAULT '0.0000',
  `ISSUE_CATEGORY` enum('ACCOUNT_CLOSURE','WRONG_TRANSFER','WITHDRAW_AMOUNT') NOT NULL,
  `CORRECT_VAN` varchar(50) DEFAULT NULL,
  `INNER_ORDER_ID` varchar(50) DEFAULT NULL,
  `INNER_ORDER_REQ_ID` varchar(50) DEFAULT NULL,
  `UTR` varchar(100) DEFAULT NULL,
  `OUTER_ORDER_ID` varchar(50) DEFAULT NULL,
  `OUTER_ORDER_REQ_ID` varchar(50) DEFAULT NULL,
  `DISPUTE_STATUS` varchar(50) NOT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `CREATED_BY` varchar(100) NOT NULL,
  `MODIFIED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `MODIFIED_BY` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`DISPUTE_ID`),
  KEY `key_ic` (`ISSUE_CATEGORY`),
  KEY `key_van` (`VAN`),
  KEY `key_ic_van` (`ISSUE_CATEGORY`,`VAN`)
) ENGINE=InnoDB AUTO_INCREMENT=1078 DEFAULT CHARSET=latin1;

